{
    "version": 2,
    "name": "shader-sandbox-backend",
    "builds": [
        {
            "src": "src/server.ts",
            "use": "@vercel/node",
            "config": {
                "runtime": "nodejs20.x"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/src/server.ts"
        }
    ]
}