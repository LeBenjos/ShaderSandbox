{
    "version": 2,
    "name": "shader-sandbox-backend",
    "builds": [
        {
            "src": "src/server.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/src/server.ts"
        }
    ],
    "functions": {
        "src/server.ts": {
            "runtime": "nodejs20.x"
        }
    }
}